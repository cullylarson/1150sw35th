{
    "name": "stratum-admin",
    "version": "0.0.1",
    "description": "The administration area of many of the Stratum projects.",
    "scripts": {
        "server:start": "NODE_ENV=production node server/app.js",
        "server:watch": "forever --watch --minUptime 2000 --spinSleepTime 5000 --watchDirectory='./server/' server/app.js",
        "server:forever": "forever --minUptime 1000 --spinSleepTime 2000 server/app.js",
        "server:lint": "esw --watch --color ./server",
        "client:clean": "rm -rf build/client",
        "client:build": "npm run client:clean && webpack --mode production",
        "client:dev": "npm run client:clean && webpack --mode development",
        "client:watch": "npm run client:clean && webpack --mode development --progress --colors --watch"
    },
    "_moduleAliases": {
        "@app": "./server"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cullylarson/1150sw35th"
    },
    "author": "Cully Larson <cully.larson@gmail.com>",
    "dependencies": {
        "date-fns": "^1.29.0",
        "escape-html": "^1.0.3",
        "express": "^4.16.3",
        "module-alias": "^2.1.0",
        "mysql": "^2.16.0",
        "nodemailer": "^6.4.16",
        "ramda": "^0.25.0",
        "twilio": "^3.43.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "copy-webpack-plugin": "^4.5.2",
        "create-react-ref": "^0.1.0",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.0",
        "eslint": "^4.19.1",
        "eslint-config-standard-preact": "^1.1.6",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-react": "^7.8.2",
        "eslint-watch": "^6.0.1",
        "forever": "^0.15.3",
        "hammerjs": "^2.0.8",
        "postcss-calc": "^6.0.1",
        "postcss-cssnext": "^3.1.0",
        "postcss-custom-media": "^6.0.0",
        "postcss-import": "^11.1.0",
